
class DisplaySubject
{
  public static void main(String[] args)
  {
    System.out.printf(" The display 1 to 100 ");
    
    for(int i=1; i<=100 ;i++)
    {
        System.out.println(i);
       if(i/3 == 0)
          System.out.println("\n is a java subject");
       if(i/4 == 0)
          System.out.println("\n is a .net subject");
       if((i/3 | i/4) == 0)
         System.out.println("\n is a web desing subject");
       
    }
  }
}