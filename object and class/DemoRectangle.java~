class DemoRectangle
{
  public static void main(String[] args)
  {
    ClassRectangle rec = new ClassRectangle(5,10);
    double area,peri;
    
    peri = rec.getPerimeter();
    area = rec.getArea();
    
    System.out.printf(" perimeter of Rectangle is :- %f",peri);
    System.out.printf("area of Rectangle is :- %f",area);
  }
}